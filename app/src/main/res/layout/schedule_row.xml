                android:layout_height="64dp"
                android:layout_below="@+id/dateRangeJI"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="8dp"
                android:contentDescription="@string/location"
                android:src="@drawable/locationlink" />

