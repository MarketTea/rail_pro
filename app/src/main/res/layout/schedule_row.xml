                android:id="@+id/imgJobDetaiLink"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/dateRangeJI"
                android:layout_alignStart="@+id/lblJobNbrJI"
                android:layout_alignBottom="@+id/jobDescJI"
                android:layout_toStartOf="@id/imgHomeMapLinkJT"
