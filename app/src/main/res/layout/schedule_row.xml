                android:layout_height="wrap_content"
                android:layout_below="@+id/jobDescJI"
                android:layout_alignStart="@+id/dateRangeJI"
                android:layout_marginTop="24dp"
                android:layout_marginBottom="4dp"
                android:orientation="horizontal"
